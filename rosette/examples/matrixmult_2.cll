int[]@{[0:49]} A[49];
int[]@{[0:49]} B[49];
int[]@{[0:49]} C[49];

int@x sum;
int[]@{[0:7]=x} a[7];

for(i from 0 to 7) {
  sum = 0;
  for(k from 0 to 7) {
    a[k] = A[7*i + k];
  }
  for(j from 0 to 7) {
    for(k from 0 to 7) {
      sum = a[k] *@x B[7*k+j];
    }
    C[7*i + j] = sum;
  }
}


void main() {
  int[]@{[0:25]=0,[25:50]=1,[50:75]=1,[75:100]=0} x[100];
  int[]@{[0:25]=2,[25:50]=3,[50:75]=4,[75:100]=5} y[100];
  int[]@{[0:25]=2,[25:50]=3,[50:75]=4,[75:100]=5} z[100];

  for(i from 0 to 100) {
    x[i] = in();
  }

  for(i from 0 to 100) {
    y[i] = in();
  }
  
  for(i from 0 to 100) {
    z[i] = x[i] +@place(z[i]) y[i];
  }

  for(i from 0 to 100) {
    out(z[i]);
  }
}

void main() {
  int[]@{[0:25],[25:50],[50:75],[75:100]} x[100];
  int[]@{[0:25],[25:50],[50:75],[75:100]} y[100];
  int[]@{[0:25],[25:50],[50:75],[75:100]} z[100];

  for(i from 0 to 100) {
    x[i] = in();
  }

  for(i from 0 to 100) {
    y[i] = in();
  }
  
  for(i from 0 to 100) {
    z[i] = x[i] +@place(z[i]) y[i];
  }

  for(i from 0 to 100) {
    out(z[i]);
  }
}

void main() {
  int::2@(0,0) x;
  int@1 res;
  int@1 add;
  int@1 temp;
  int::2@(0,0) g2;

  res = 0;
  add = 32768; 
  
  x::0 = 1; // high
  x::1 = 0; // low
  for(i from 0 to 16) {
    temp = res | add;
    g2 = temp *:2 temp;
    g2::0 = (g2::0 << 2) + ((g2::1 >> 16) & 3);
    g2::1 = g1::1 & 65535;
    if(x::0 > g2::0) {
      res = temp;
    } else {
      if(x::0 == g2::0) {
        if(x::1 >= g2::1) {
          res = temp;
        }
      }
    }
    out(res);
    add = add >> 1;
  }
  //out(res);
}

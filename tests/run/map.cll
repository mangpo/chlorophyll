int ssd(int a, int b) {
  int temp;
  temp = a -@place(temp) b;
  return temp *@place(temp) temp;
}

void main() {
  int[]@{[0:5]=0,[5:10]=1} x[10] = {7 , 3 , 7 , 9 , 0 , 1 , 6 , 4 , 9 , 9};
  int[]@{[0:5],[5:10]} y[10] = {4 , 2 , 10 , 6 , 5 , 6 , 1 , 5 , 6 , 7};
  int[]@{[0:5],[5:10]} z[10];
  z = map(ssd, x, y);
  for(i from 0 to 10) {
    out(z[i]);
  }
}
